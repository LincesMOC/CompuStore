<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fiuady.android.compustore">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CategoriesActivity"
            android:label="@string/categories_title" />
        <activity android:name=".ProductsActivity"
            android:label="Productos"
            ></activity>
        <activity android:name=".ClientsActivity"></activity>

        <activity android:name=".ReportsActivity"
            android:label="@string/reports_title"
            ></activity>

        <activity android:name=".MisssingProductsActivity"
            android:label="@string/reports_title"
            ></activity>
    </application>

</manifest>